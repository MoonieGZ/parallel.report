{
  "name": "@solely/parallel",
  "version": "3.0.0",
  "type": "module",
  "author": "Chloe Arciniega <solely@riseup.net>",
  "license": "zlib",
  "private": true,
  "scripts": {
    "astro:check": "astro check",
    "build": "yarn build:check && astro build",
    "build:check": "yarn astro:check && tsc --noEmit",
    "dev": "yarn start:dev",
    "format:prettier": "prettier --w --plugin-search-dir=. --cache .",
    "lint:eslint": "eslint --fix src/**/*.{ts,astro}",
    "start": "astro preview",
    "start:dev": "astro dev",
    "test": "eslint . --ext .ts,.js,.astro && prettier --c .",
    "wrangler:dev": "wrangler pages dev --proxy 3000 -- yarn dev",
    "wrangler:start": "wrangler pages dev --proxy 3000 -- yarn start"
  },
  "repository": "https://github.com/solelychloe/parallel.report",
  "packageManager": "yarn@3.6.0",
  "dependencies": {
    "@astrojs/mdx": "^0.19.6",
    "@astrojs/prefetch": "^0.2.3",
    "@astrojs/tailwind": "^3.1.3",
    "@fontsource/atkinson-hyperlegible": "^5.0.2",
    "@fontsource/inter": "^5.0.2",
    "astro": "^2.5.7",
    "astro-compress": "^1.1.47",
    "astro-iconify": "^1.2.0",
    "astro-seo": "^0.7.2",
    "rehype-external-links": "^2.1.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/eslint": "^8.40.0",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "astro-eslint-parser": "^0.14.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-plugin-astro": "^0.27.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "typescript": "^5.1.3"
  }
}
