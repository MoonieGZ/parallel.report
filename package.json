{
  "name": "@solely/parallel",
  "version": "3.0.0",
  "type": "module",
  "author": "Chloe Arciniega <solely@riseup.net>",
  "license": "zlib",
  "private": true,
  "scripts": {
    "astro:check": "astro check",
    "build": "yarn build:check && astro build",
    "build:check": "yarn astro:check && tsc --noEmit",
    "dev": "yarn start:dev",
    "format:prettier": "prettier --w --plugin-search-dir=. .",
    "lint:eslint": "eslint --fix src/**/*.{ts,astro}",
    "start": "astro preview",
    "start:dev": "astro dev",
    "test": "eslint src/**/*.{ts,astro} && prettier --c .",
    "wrangler:dev": "wrangler pages dev --proxy 3000 -- yarn dev",
    "wrangler:start": "wrangler pages dev --proxy 3000 -- yarn start"
  },
  "repository": "https://github.com/solelychloe/parallel.report",
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@astrojs/mdx": "^0.18.2",
    "@astrojs/prefetch": "^0.2.1",
    "@astrojs/sitemap": "^1.2.1",
    "@astrojs/tailwind": "^3.1.1",
    "@fontsource/atkinson-hyperlegible": "^4.5.11",
    "@fontsource/inter": "^4.5.15",
    "astro": "^2.1.7",
    "astro-compress": "^1.1.35",
    "astro-iconify": "^1.2.0",
    "astro-seo": "^0.7.2",
    "rehype-external-links": "^2.0.1",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/eslint": "^8.21.3",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "astro-eslint-parser": "^0.13.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-plugin-astro": "^0.26.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "typescript": "^5.0.2"
  }
}
